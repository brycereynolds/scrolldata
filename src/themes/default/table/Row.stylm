@import '../../../table/Row.styl'

.row
    border-top 1px solid rgba(0, 0, 0, 0.12)

.expanded .row
    border-bottom 1px solid rgba(0, 0, 0, 0.12)
    min-height 50px


.expandable
    border-bottom 1px solid rgba(0, 0, 0, 0.12)
    min-height 50px

.notIntersecting
    color transparent
    font-size .1
    white-space nowrap
    min-width 100%;
    max-width 100%;
