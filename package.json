{
  "name": "scrolldata",
  "version": "4.1.0",
  "description": "Table and virtualization help for tabular data",
  "main": "lib",
  "source": "src",
  "dependencies": {},
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "BSD",
  "devDependencies": {
    "chai": "^4.1.2",
    "hint.css": "^2.5.0",
    "mrbuilder": "^2.0.3",
    "mrbuilder-plugin-clean": "^2.0.3",
    "mrbuilder-plugin-css": "^2.0.3",
    "mrbuilder-plugin-emeth": "^2.0.3",
    "mrbuilder-plugin-html": "^2.0.3",
    "mrbuilder-plugin-stylus": "^2.0.3",
    "mrbuilder-preset-app": "^2.0.3",
    "mrbuilder-preset-lib": "^2.0.3",
    "mrbuilder-preset-test": "^2.0.3"
  },
  "peerDependencies": {
    "emeth": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "karma": "mrbuilder",
    "test": "mrbuilder",
    "start": "mrbuilder",
    "demo": "MRBUILDER_ENV=demo mrbuilder-webpack && git add docs",
    "gh-pages": "git subtree push --prefix docs origin gh-pages",
    "build": "mrbuilder",
    "preversion": "$npm_execpath run build && $npm_execpath run test"
  },
  "mrbuilder": {
    "plugins": [
      [
        "mrbuilder-plugin-css",
        {
          "useNameHash": "style.css"
        }
      ],
      "mrbuilder-plugin-emeth",
      "mrbuilder-plugin-stylus",
      "mrbuilder-plugin-clean"
    ],
    "env": {
      "demo": {
        "plugins": [
          [
            "mrbuilder-plugin-webpack",
            {
              "demo": "./docs",
              "entry": {
                "index": "./public/index"
              }
            }
          ],
          [
            "mrbuilder-plugin-html",
            {
              "exported": true
            }
          ]
        ]
      },
      "development": {
        "plugins": [
          [
            "mrbuilder-plugin-html",
            {
              "exported": true
            }
          ]
        ]
      },
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "useStyleLoader": true
            }
          ],
          [
            "mrbuilder-plugin-hot",
            false
          ],
          "mrbuilder-plugin-stylus"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
