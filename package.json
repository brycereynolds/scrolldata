{
  "name": "scrolldata",
  "version": "4.0.2",
  "description": "Table and virtualization help for tabular data",
  "main": "lib",
  "source": "src",
  "dependencies": {},
  "author": "Justin Spears <speajus@gmail.com>",
  "license": "BSD",
  "devDependencies": {
    "chai": "^4.1.2",
    "emeth": "^1.0.0",
    "hint.css": "^2.5.0",
    "mrbuilder": "^0.5.0",
    "mrbuilder-plugin-stylus": "^0.5.0"
  },
  "peerDependencies": {
    "emeth": "*",
    "prop-types": "*",
    "react": "*",
    "react-dom": "*"
  },
  "scripts": {
    "karma": "mrbuilder-karma",
    "test": "mrbuilder-karma",
    "start": "mrbuilder-webpack-dev-server --entry index=public/index",
    "demo": "mrbuilder-clean docs && mrbuilder-webpack --demo ./docs  --entry index=public/index && git add docs",
    "gh-pages": "git subtree push --prefix docs origin gh-pages",
    "build": "mrbuilder-webpack",
    "preversion": "$npm_execpath run build && $npm_execpath run test"
  },
  "mrbuilder": {
    "plugins": [
      "mrbuilder-plugin-css",
      "mrbuilder-plugin-stylus"
    ],
    "env": {
      "test": {
        "plugins": [
          [
            "mrbuilder-plugin-css",
            {
              "isUseStyleLoader": true
            }
          ],
          "mrbuilder-plugin-stylus"
        ]
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org"
  }
}
